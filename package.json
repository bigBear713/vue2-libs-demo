{
  "name": "vue2-libs-demo",
  "version": "2.0.0",
  "description": "使用vue2写的几个简单组件，此工程包含组件源码和使用示例",
  "author": "",
  "license": "MIT",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env BABEL_ENV=development webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "build": "cross-env NODE_ENV=development env_config=prod node build/build.js",
    "build:prod": "cross-env NODE_ENV=production env_config=prod node build/build.js",
    "build:sit": "cross-env NODE_ENV=production env_config=sit node build/build.js",
    "package:style": "gulp --gulpfile build/package/build-style.js",
    "package:common": "webpack --config build/package/webpack.dev.conf.js",
    "package:min": "webpack --config build/package/webpack.prod.conf.js",
    "npm_pack:vue2-libs": "cp projects/vue2-libs/package.json node_modules/vue2-libs/ && cp projects/vue2-libs/changelog.md node_modules/vue2-libs/ && cd node_modules/vue2-libs && npm pack && cd ../../ && mv node_modules/vue2-libs/*.tgz libs/vue2-libs/",
    "package:vue2-libs": "rm -rf node_modules/vue2-libs/* && npm run package:style && npm run package:common && npm run package:min && npm run npm_pack:vue2-libs",
    "lint": "eslint --ext .js,.vue src",
    "test": "npm run lint",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml"
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "vue"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wjx774326739/vue2-libs-demo.git"
  },
  "bugs": {
    "url": "https://github.com/wjx774326739/vue2-libs-demo/issues"
  },
  "dependencies": {
    "normalize.css": "7.0.0",
    "vue": "2.5.17",
    "vue-router": "3.0.2",
    "vue2-libs": "file:libs/vue2-libs/vue2-libs-2.0.0.tgz",
    "vuex": "3.0.1"
  },
  "devDependencies": {
    "autoprefixer": "8.5.0",
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "^6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.6",
    "babel-helper-vue-jsx-merge-props": "2.0.3",
    "babel-loader": "7.1.5",
    "babel-plugin-dynamic-import-node": "2.0.0",
    "babel-plugin-syntax-jsx": "6.18.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-plugin-transform-vue-jsx": "3.7.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-stage-2": "6.24.1",
    "chalk": "2.4.1",
    "compression-webpack-plugin": "^1.1.12",
    "connect": "^3.6.6",
    "copy-webpack-plugin": "4.5.2",
    "cross-env": "5.2.0",
    "css-loader": "1.0.0",
    "eslint": "4.19.1",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-loader": "2.0.0",
    "eslint-plugin-vue": "4.7.1",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "1.7.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-clean-css": "^3.10.0",
    "gulp-less": "^4.0.1",
    "gulp-rename": "^1.4.0",
    "hash-sum": "1.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "4.0.0-alpha",
    "husky": "0.14.3",
    "less": "^2.7.3",
    "less-loader": "^4.0.6",
    "lint-staged": "7.2.2",
    "mini-css-extract-plugin": "0.4.1",
    "node-notifier": "5.2.1",
    "node-sass": "^4.7.2",
    "optimize-css-assets-webpack-plugin": "5.0.0",
    "ora": "3.0.0",
    "path-to-regexp": "2.4.0",
    "portfinder": "1.0.13",
    "postcss-import": "11.1.0",
    "postcss-loader": "2.1.6",
    "postcss-url": "7.3.2",
    "rimraf": "2.6.2",
    "sass-loader": "7.0.3",
    "script-ext-html-webpack-plugin": "2.0.1",
    "script-loader": "0.7.2",
    "semver": "5.5.0",
    "serve-static": "1.13.2",
    "shelljs": "0.8.2",
    "style-loader": "^0.20.2",
    "svg-sprite-loader": "3.8.0",
    "svgo": "1.0.5",
    "uglifyjs-webpack-plugin": "1.2.7",
    "url-loader": "1.0.1",
    "vue-loader": "^15.5.0",
    "vue-style-loader": "4.1.2",
    "vue-template-compiler": "2.5.17",
    "webpack": "4.16.5",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.5",
    "webpack-merge": "4.1.4"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
